<template name="mona-base">
  <template tt:for="{{root.children}}" tt:key="*this" is="{{'mona-' + root.nodes[item].type}}" data="{{node: root.nodes[item]}}" />
</template>
<%for (let name of ejsParamsMap.keys()) {%>
<template name="mona-<%=name%>">
  <<%=name%> 
    <%_for(let i in ejsParamsMap.get(name).alias){-%>
    <%_if(ejsParamsMap.get(name).defaultProps[i] !== undefined){-%>
    <%=ejsParamsMap.get(name).alias[i]%>="{{node.props['<%=i%>'] === undefined ? <%-ejsParamsMap.get(name).defaultProps[i]%> : node.props['<%=i%>']}}" 
    <%_} else if(i !== 'id'){-%>
    <%=ejsParamsMap.get(name).alias[i]%>="{{node.props['<%=i%>']}}" 
    <%_}}-%>
    id="{{node.key}}" 
  >
    <template 
      is="{{ 'mona-' + node.nodes[item].type }}" 
      tt:for="{{ node.children }}"
      tt:key="{{ item.key }}"
      data="{{ node: node.nodes[item] }}" 
    />
  </<%=name%>>
</template><%};-%>
<template name="mona-ptext">
  <block>{{ node.text }}</block>
</template>