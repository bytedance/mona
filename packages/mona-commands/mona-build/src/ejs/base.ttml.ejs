<template name="mona-base">
  <block tt:for="{{root.children}}" tt:key="*this">
    <template is="{{'mona-' + root.nodes[item].type}}" data="{{node: root.nodes[item]}}" />
  </block>
</template>

<% for (let name of aliasMap.keys()) { %>
<%-include(templatePath+'/'+name+'.ejs', {props: aliasMap.get(name)}); %>
<% }; %>

<template name="mona-ptext">
  <block>{{ node.text }}</block>
</template>
