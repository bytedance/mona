<template name="mona-icon">
  <icon 
    <%_ for (let i in props) { -%>
    <%_ if(defaultProps[i] !== undefined)  {  -%>
    <%=props[i]%>="{{node.props['<%=i%>'] === undefined ? <%=defaultProps[i]%> : node.props['<%=i%>']}}" 
    <%_   } else {  -%>
    <%=props[i]%>="{{node.props['<%=i%>']}}" 
    <%_ }
    } -%>
  >
    <template 
      is="{{ 'mona-' + node.nodes[item].type }}" 
      tt:for="{{ node.children }}"
      tt:key="{{ item.key }}"
      data="{{ node: node.nodes[item] }}" />
  </icon>
</template>