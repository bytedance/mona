<template name="mona-picker-view">
  <picker-view 
    <%_ for (let i in props) { -%>
    <%_ if(defaultProps[i] !== undefined)  {  -%>
    <%=props[i]%>="{{node.props['<%=i%>'] === undefined ? <%-defaultProps[i]%> : node.props['<%=i%>']}}" 
    <%_   } else {  -%>
    <%=props[i]%>="{{node.props['<%=i%>']}}" 
    <%_ }
    } -%>
  >
    <block a:for="{{node.children}}" key="{{item.id}}">
      <picker-view-column>
        <view a:for="{{item.children}}">
          <template is="{{ 'mona-' + node.nodes[item].type }}" data="{{root: item}}" />
        </view>
      </picker-view-column>
    </block>
  </picker-view>
</template>