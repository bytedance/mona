<template name="mona-live-play">
  <live-play <%_ for (let i in props) { -%>
    <%_ if(defaultProps[i] !== undefined) { -%> <%=props[i]%>="{{
      node.props['<%=i%>'] === undefined ? defaultProps[i] : node.props['<%=i%>']
    }}" <%_ } else { -%> <%=props[i]%>="{{ node.props['<%=i%>'] }}" <%_ } } -%> >
  </live-play>
</template>
