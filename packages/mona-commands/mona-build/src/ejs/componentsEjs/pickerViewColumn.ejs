<template name="mona-picker-view-column">
  <picker-view-column <% for (let i in props) { %>
    <%=props[i]%>="{{node.props['<%=i%>']}}" <% } %>
  >
    <template 
      is="{{ 'mona-' + node.nodes[item].type }}" 
      tt:for="{{ node.children }}"
      tt:key="{{ item.key }}"
      data="{{ node: node.nodes[item] }}" />
  </picker-view-column>
</template>