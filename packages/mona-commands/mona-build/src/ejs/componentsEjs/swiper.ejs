<template name="mona-swiper">
  <swiper <% for (let i in props) { %>
    <%=props[i]%>="{{node.props['<%=i%>']}}" <% } %>
  >
    <block >
      <template 
      is="{{ 'mona-' + node.nodes[item].type }}" 
      tt:for="{{ node.children }}"
      tt:key="{{ item.key }}"
      data="{{ node: node.nodes[item] }}" />
    </block>
  </swiper>
</template>
