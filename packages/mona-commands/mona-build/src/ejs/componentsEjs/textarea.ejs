<template name="mona-textarea">
  <textarea <% for (let i in props) { %>
    <%=props[i]%>="{{ node.props['<%=i%>'] }}" <% } %>
  >
    <template 
      is="{{ 'mona-' + node.nodes[item].type }}" 
      tt:for="{{ node.children }}"
      tt:key="{{ item.key }}"
      data="{{ node: node.nodes[item] }}" />
  </textarea>
</template>
