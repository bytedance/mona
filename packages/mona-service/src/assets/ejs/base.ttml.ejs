<template name="mona-base">
  <template tt:for="{{root.<%=RENDER_NODE.COMPLIER_CHILDREN%>}}" tt:key="{{ item.<%=RENDER_NODE.COMPLIER_KEY%> }}" is="{{'mona-' + root.<%=RENDER_NODE.COMPLIER_NODES%>[item].<%=RENDER_NODE.COMPLIER_TYPE%>}}" data="{{node: root.<%=RENDER_NODE.COMPLIER_NODES%>[item]}}" />
</template>
<%for (let name of miniComponents.keys()) {%>
<%_const itemInfo = miniComponents.get(name)-%>
<template name="mona-<%=name%>">
  <<%=itemInfo.miniName%> 
    <%_for(let i in itemInfo.alias){-%>
    <%_if(itemInfo.defaultProps[i] !== undefined){-%>
    <%=itemInfo.alias[i]%>="{{node.<%=RENDER_NODE.COMPLIER_PROPS%>['<%=i%>'] === undefined ? <%-itemInfo.defaultProps[i]%> : node.<%=RENDER_NODE.COMPLIER_PROPS%>['<%=i%>']}}" 
    <%_} else{-%>
    <%=itemInfo.alias[i]%>="{{node.<%=RENDER_NODE.COMPLIER_PROPS%>['<%=i%>']}}" 
    <%_}}-%>
  >
  <% if(!voidChildrenElements.has(name)) { -%>
    <template 
      is="{{ 'mona-' + node.<%=RENDER_NODE.COMPLIER_NODES%>[item].<%=RENDER_NODE.COMPLIER_TYPE%> }}" 
      tt:for="{{ node.<%=RENDER_NODE.COMPLIER_CHILDREN%> }}"
      tt:key="{{ item.<%=RENDER_NODE.COMPLIER_KEY%> }}"
      data="{{ node: node.<%=RENDER_NODE.COMPLIER_NODES%>[item] }}" 
    />
  <%_}-%></<%=itemInfo.miniName%>>
</template><%};-%>


<%for (let item of nativeComponents.values()) {%>
  <template name="mona-<%=item.id%>">
    <<%=item.name%> 
      <%_for(let i in item.props){-%>
        <%=i%>="{{node.<%=RENDER_NODE.COMPLIER_PROPS%>['<%=item.props[i]%>']}}" 
      <%}-%>
    >
      <template 
        is="{{ 'mona-' + node.<%=RENDER_NODE.COMPLIER_NODES%>[item].<%=RENDER_NODE.COMPLIER_TYPE%> }}" 
        tt:for="{{ node.<%=RENDER_NODE.COMPLIER_CHILDREN%> }}"
        tt:key="{{ item.<%=RENDER_NODE.COMPLIER_KEY%> }}"
        data="{{ node: node.<%=RENDER_NODE.COMPLIER_NODES%>[item] }}" 
      />
     </<%=item.name%>>
  </template>
<%};-%>

<template name="mona-<%=PTextName%>">
  <block>{{ node.<%=RENDER_NODE.COMPLIER_TEXT%> }}</block>
</template>


